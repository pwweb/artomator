{
    "name": "pwweb/artomator",
    "description": "PWWEB Artomator",
    "license": "MIT",
    "authors": [{
        "name": "Richard Browne",
        "email": "richard@pw-websolutions.com",
        "homepage": "https://www.pw-websolutions.com"
    }],
    "homepage": "https://github.com/pwweb/artomator",
    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
