    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0.0",
        "laracasts/flash": "~3.0",
        "laravelcollective/html": "^6.0",
        "doctrine/dbal": "~2.3",
        "infyomlabs/coreui-templates": "6.0.x-dev",
        "infyomlabs/laravel-generator": "6.0.x-dev",
        "nuwave/lighthouse": "^4.9",
        "funkjedi/composer-include-files": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9",
        "mll-lab/laravel-graphql-playground": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PWWEB\\Artomator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PWWEB\\Artomator\\ArtomatorServiceProvider"
            ]
        },
        "include_files": [
            "src/helpers.php"
        ]
    }
}