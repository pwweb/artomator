
    create$MODEL_NAME$(
        $SCHEMA$
    ): $MODEL_NAME$ @create(model: "$NAMESPACE_GRAPHQL_MODEL$\\$MODEL_NAME$")

    update$MODEL_NAME$(
        id: ID
        $SCHEMA$
    ): $MODEL_NAME$ @update(model: "$NAMESPACE_GRAPHQL_MODEL$\\$MODEL_NAME$")

    delete$MODEL_NAME$(
        id: ID!
    ): $MODEL_NAME$ @delete(model: "$NAMESPACE_GRAPHQL_MODEL$\\$MODEL_NAME$")
